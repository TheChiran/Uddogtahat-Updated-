<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <meta name="author" content="Dashboard">
    <meta name="keyword" content="Dashboard, Bootstrap, Admin, Template, Theme, Responsive, Fluid, Retina">
    <title><%=title%> Admin</title>


    <!-- Bootstrap core CSS -->
    <link href="/admin/lib/bootstrap/css/bootstrap.min.css" rel="stylesheet">
    <!--external css-->
    <link href="/admin/lib/font-awesome/css/font-awesome.css" rel="stylesheet" />
    <!-- Custom styles for this template -->
    <link href="/admin/css/style.css" rel="stylesheet">
    <link href="/admin/css/style-responsive.css" rel="stylesheet">
    <script>
        function submit(){
            var officeHouseNumber = $('#officeHouseNumber').val();
            var officeFlatNumber = $('#officeFlatNumber').val();
            var officeRoadNumber = $('#officeRoadNumber').val();
            var officeArea = $('#officeArea').val();
            var officeEmail = $('#officeEmail').val();
            var officeTelephoneNumber = $('#officeTelephoneNumber').val();
            var officeMobileNumber = $('#officeMobileNumber').val();
            if(officeHouseNumber =='' || officeFlatNumber =='' || officeRoadNumber =='' || officeArea ==''
            || officeEmail =='' || officeTelephoneNumber =='' || officeMobileNumber ==''){
                document.getElementById('statusBox').style="display: block";
                document.getElementById('status').style="padding: 15px 80px;background: #d9534f!important;color: #fff;font-weight: bold";
                document.getElementById('status').innerHTML="Please Fill all the fields";
            }else{
                $.ajax({
                    type: 'post',
                    url:'/contact/contact-page',
                    data:{
                        houseNumber: officeHouseNumber,
                        flatNumber: officeFlatNumber,
                        road: officeRoadNumber,
                        area: officeArea,
                        email: officeEmail,
                        telephone: officeTelephoneNumber,
                        mobile: officeMobileNumber
                    },
                    success: function(response){
                        $('#officeHouseNumber').val('');
                        $('#officeFlatNumber').val('');
                        $('#officeRoadNumber').val('');
                        $('#officeArea').val('');
                        $('#officeEmail').val('');
                        $('#officeTelephoneNumber').val('');
                        $('#officeMobileNumber').val('');
                        document.getElementById('statusBox').style="display: block";
                        $("#status").empty().text(response);
                    }
                })
            }


            return false;
        }
    </script>

</head>

<body>
<section id="container">
    <!-- **********************************************************************************************************************************************************
        TOP BAR CONTENT & NOTIFICATIONS
        *********************************************************************************************************************************************************** -->
    <!--header start-->
    <header class="header black-bg">
        <div class="sidebar-toggle-box">
            <div class="fa fa-bars tooltips" data-placement="right" data-original-title="Toggle Navigation"></div>
        </div>
        <!--logo start-->
        <a href="index.html" class="logo"><b><%=title%> Dashboard</b></a>
        <!--logo end-->
        <div class="top-menu">
            <ul class="nav pull-right top-menu">
                <li><a class="logout" href="/admin/logout">Logout</a></li>
            </ul>
        </div>
    </header>
    <!--header end-->
    <!-- **********************************************************************************************************************************************************
        MAIN SIDEBAR MENU
        *********************************************************************************************************************************************************** -->
    <!--sidebar start-->
    <aside>
        <div id="sidebar" class="nav-collapse ">
            <!-- sidebar menu start-->
            <ul class="sidebar-menu" id="nav-accordion">
                <p class="centered"><a href=""><img src="/uploads/admin/<%=userimage%>" class="img-circle" style="width: 100px;height: 100px"></a></p>
                <h5 class="centered"><%=username%></h5>

                <% include ../partials/dashboardnavbar %>
            </ul>
            <!-- sidebar menu end-->
        </div>
    </aside>
    <!--sidebar end-->
    <!-- **********************************************************************************************************************************************************
        MAIN CONTENT
        *********************************************************************************************************************************************************** -->
    <!--main content start-->
    <section id="main-content">
        <section class="wrapper">
            <!-- page start-->
            <div class="row mt" style="margin: 25px 0px">
                <div class="col-md-4"></div>
                <div class="col-md-4">
                    <div id="statusBox" style="display: none">
                        <span id="status" class="alert-success" style="padding: 15px 80px;background: #D4EDDA!important;color: #155724;font-weight: bold"></span>
                    </div>
                </div>
                <div class="col-md-4"></div>
            </div>
            <div class="row mt">
                <div class="col-sm-3"></div>
                <div class="col-sm-6">
                    <h2 style="text-transform: uppercase">Office Address</h2>
                    <form
                          class="form-horizontal style-form"
                            name="contactForm" novalidate>
                        <div class="form-row">
                            <div class="form-group">
                                <label class="col-sm-2 col-sm-2 control-label">House Number</label>
                                <div class="col-sm-6">
                                    <input type="text" class="form-control"
                                           name="officeHouseNumber" id="officeHouseNumber"
                                           required>
                                </div>
                            </div>
                            <div class="form-group">
                                <label class="col-sm-2 col-sm-2 control-label">Flat Number</label>
                                <div class="col-sm-6">
                                    <input type="text" class="form-control"
                                           name="officeFlatNumber" id="officeFlatNumber" required>
                                </div>
                            </div>
                            <div class="form-group">
                                <label class="col-sm-2 col-sm-2 control-label">Road </label>
                                <div class="col-sm-6">
                                    <input type="text" class="form-control"
                                           name="officeRoadNumber" id="officeRoadNumber" required>
                                </div>
                            </div>
                            <div class="form-group">
                                <label class="col-sm-2 col-sm-2 control-label">Office Area</label>
                                <div class="col-sm-6">
                                    <input type="text" class="form-control"
                                           name="officeArea" id="officeArea" required>
                                </div>
                            </div>
                            <div class="form-group">
                                <label class="col-sm-2 col-sm-2 control-label">Office Email</label>
                                <div class="col-sm-6">
                                    <input type="email" class="form-control"
                                           name="officeEmail" id="officeEmail"
                                           required>
                                </div>
                            </div>
                            <div class="form-group">
                                <label class="col-sm-2 col-sm-2 control-label">Office Telephone number</label>
                                <div class="col-sm-6">
                                    <input type="number" class="form-control"
                                           name="officeTelephoneNumber" id="officeTelephoneNumber" required>
                                </div>
                            </div>
                            <div class="form-group">
                                <label class="col-sm-2 col-sm-2 control-label">Office Mobile Phone</label>
                                <div class="col-sm-6">
                                    <input type="number" class="form-control"
                                           name="officeMobileNumber" id="officeMobileNumber" required>
                                </div>
                            </div>
                            <div class="form-group">
                                <div class="col-md-3">
                                    <a href="" class="btn btn-md btn-success" onclick="return submit()">Submit</a>
                                </div>
                                <div class="col-md-4">

                                </div>
                            </div>
                        </div>

                    </form>
                </div>
                <div class="col-sm-3">
                </div>
            </div>
        </section>
        <!-- /wrapper -->
    </section>
    <!-- /MAIN CONTENT -->
    <!--main content end-->
    <!--footer start-->
    <footer class="site-footer">
        <div class="text-center">
            <div class="credits">
                Copyright &copy;<script>document.write(new Date().getFullYear());</script> All rights reserved | by <a href="http://softcareit.com/" target="_blank">SoftcareIT</a>
            </div>
            <a href="" class="go-top">
                <i class="fa fa-angle-up"></i>
            </a>
        </div>
    </footer>
    <!--footer end-->
</section>
<!-- js placed at the end of the document so the pages load faster -->


<script src="/admin/lib/jquery/jquery.min.js"></script>
<script src="/admin/lib/bootstrap/js/bootstrap.min.js"></script>
<script class="include" type="text/javascript" src="/admin/lib/jquery.dcjqaccordion.2.7.js"></script>
<script src="/admin/lib/jquery.scrollTo.min.js"></script>
<script src="/admin/lib/jquery.nicescroll.js" type="text/javascript"></script>
<!--common script for all pages-->
<script src="/admin/lib/common-scripts.js"></script>
<script src="http://malsup.github.com/jquery.form.js"></script>

<!--script for this page-->
</body>
</html>
