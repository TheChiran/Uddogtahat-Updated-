            <div class="row mt">
                <div class="col-md-2"></div>
                <div class="col-md-8">
                    <h2 align="center">Company Information</h2>
                    <form method="post" enctype="multipart/form-data"
                            action="/admin/member-register-2">
                        <table class="table">
                            <tr>
                                <td>Company Name</td>
                                <td>
                                    <input type="text" class="form-control" id="inputCompanyName"
                                           placeholder="Enter Company Name"
                                           name="companyname" >
                                    <!--primary key Applicant mobile -->
                                    <input type="text" value="<%=mobile%>"
                                           name="primarymobile" hidden>
                                    <input type="text"
                                           value="<%=serial%>"
                                           name="primaryserial" hidden>
                                </td>
                            </tr>
                            <tr>
                                <td>Address 1</td>
                                <td>
                                    <input type="text" class="form-control" id="inputAddress1"
                                           placeholder="Enter Company Name Address"
                                           name="companyaddress1">
                                </td>
                            </tr>
                            <tr>
                                <td>Address 2</td>
                                <td>
                                    <input type="text" class="form-control" id="inputAddress2"
                                           placeholder="Enter Company Name Address"
                                           name="companyaddress2">
                                </td>
                            </tr>
                            <tr>
                                <td>Telephone</td>
                                <td>
                                    <input type="number" class="form-control" id="inputCompanyTelephone"
                                           placeholder="Telephone"
                                           name="telephone" >
                                </td>
                            </tr>
                            <tr>
                                <td>Cell Phone</td>
                                <td>
                                    <input type="text" class="form-control" id="inputCompanyCell"
                                           placeholder="Cell Number"
                                           name="cellphone">
                                </td>
                            </tr>
                            <tr>
                                <td>Office Email Address</td>
                                <td>
                                    <input type="email" class="form-control"
                                           id="inputCompanyEmail"
                                           placeholder="Company Email"
                                           name="email">
                                </td>
                            </tr>
                            <tr>
                                <td>Office Website</td>
                                <td>
                                    <input type="text" class="form-control"
                                           id="inputOfficeWebsite"
                                           placeholder="Company's Website"
                                           name="website">
                                </td>
                            </tr>
                            <tr>
                                <td>Company Logo</td>
                                <td>
                                    Upload Logo
                                </td>
                            </tr>
                            <tr>
                                <td></td>
                                <td>
                                    <input type="file" class="form-control-file"
                                           id="companyLogo" name="companylogo">
                                </td>
                            </tr>
                            <tr>
                                <td></td>
                                <td>
