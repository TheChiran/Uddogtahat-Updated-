    <style>
        .dropbtn {
            font-size: 12px;
            border: none;
            background: none;
            text-transform: uppercase;
            color: #0b1033;
            letter-spacing: 3.5px;
            padding-left: 14px;
            padding-right: 10px;
            font-weight: 600;
        }
        .dropbtn:hover{
            cursor: pointer;
            color: #fff;
        }
        .dropdown {
            display: inline-block;
            margin-top: 43px !important;
        }
        .dropdown-content {
            display: none;
            position: absolute;
            background-color: #28cc28;
            min-width: 160px;
            z-index: 1;
        }

        .dropdown-content a {
            color: #fff;
            padding: 12px 16px;
            text-decoration: none;
            display: block;
        }
        .dropdown-content:hover{
            cursor: pointer;
            color: #fff;
        }
        .dropdown-content a:hover {background-color: #ddd;
            color: #fff;
        }

        .dropdown:hover .dropdown-content {display: block;}

        .dropdown:hover .dropbtn {
            background-color: #28cc28;
            color: #fff;
        }

    </style>

    <script>
        function submit(){
            //document.getElementById('msg').innerHTML="You Clicked me!";
            var memberno = $('#inputMemberNo').val();
            //var serialID = $('#serialID').val();
            var mobileID= $('#mobileID').val();
            if(memberno ==''){
                memberno='';
            }else{
                memberno=$('#inputMemberNo').val();
            }
            $.ajax({
                type:'post',
                url:'/applicant/register-form-2-submit',
                data:{
                    membershipno: memberno,
                    mobile: mobileID
                    //serial: serialID
                },
                success:function(response){
                    if(response){
                        $('#inputMemberNo').val('');
                        document.getElementById('msgbox').style="display:block";
                        document.getElementById('msg').innerHTML=response;
                    }

                }
            })
            return false;
        }
    </script>
</head>

