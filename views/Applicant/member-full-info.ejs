<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <meta name="author" content="Dashboard">
    <meta name="keyword" content="Dashboard, Bootstrap, Admin, Template, Theme, Responsive, Fluid, Retina">
    <title><%=title%> Admin</title>



    <!-- Bootstrap core CSS -->
    <link href="/admin/lib/bootstrap/css/bootstrap.min.css" rel="stylesheet">
    <!--external css-->
    <link href="/admin/lib/font-awesome/css/font-awesome.css" rel="stylesheet" />
    <!-- Custom styles for this template -->
    <link href="/admin/css/style.css" rel="stylesheet">
    <link href="/admin/css/style-responsive.css" rel="stylesheet">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/4.1.3/css/bootstrap.css">
    <link rel="stylesheet" href="https://cdn.datatables.net/1.10.20/css/dataTables.bootstrap4.min.css">
    <!-- =======================================================
      Template Name: Dashio
      Template URL: https://templatemag.com/dashio-bootstrap-admin-template/
      Author: TemplateMag.com
      License: https://templatemag.com/license/
    ======================================================= -->
</head>

<body style="font-size: 15px">
<section id="container">
    <!-- **********************************************************************************************************************************************************
        TOP BAR CONTENT & NOTIFICATIONS
        *********************************************************************************************************************************************************** -->
    <!--header start-->
    <header class="header black-bg">
        <div class="sidebar-toggle-box">
            <div class="fa fa-bars tooltips" data-placement="right" data-original-title="Toggle Navigation"></div>
        </div>
        <!--logo start-->
        <a href="index.html" class="logo"><b><%=title%> Dashboard</b></a>
        <!--logo end-->
        <div class="top-menu">
            <ul class="nav pull-right top-menu">
                <li><a class="logout" href="/admin/logout">Logout</a></li>
            </ul>
        </div>
    </header>
    <!--header end-->
    <!-- **********************************************************************************************************************************************************
        MAIN SIDEBAR MENU
        *********************************************************************************************************************************************************** -->
    <!--sidebar start-->
    <aside>
        <div id="sidebar" class="nav-collapse ">
            <!-- sidebar menu start-->
            <ul class="sidebar-menu" id="nav-accordion">
                <p class="centered"><a href=""><img src="/uploads/admin/<%=userimage%>" class="img-circle" style="width: 100px;height: 100px"></a></p>
                <h5 class="centered"><%=username%></h5>

                <% include ../partials/dashboardnavbar %>
            </ul>
            <!-- sidebar menu end-->
        </div>
    </aside>
    <!--sidebar end-->
    <!-- **********************************************************************************************************************************************************
        MAIN CONTENT
        *********************************************************************************************************************************************************** -->
    <!--main content start-->
    <section id="main-content">
        <section class="wrapper">
            <!-- page start-->
            <div class="row mt">
                <div class="col-sm-2">
                </div>
                <div class="col-sm-12">
                    <table id="member_full_info" class="table table-striped table-bordered" style="width:100%">
                        <thead>
                        <tr>
                            <th style="text-align: center">Mailing Address</th>
                        </tr>
                        </thead>
                        <tbody>
                        <% for(var i =0;i<mailing_address.length;i++){%>
                        <tr>
                            <td>
                                <table class="table table-bordered">
                                    <tr>
                                        <th>City</th>
                                        <th>State</th>
                                        <th>Road</th>
                                        <th>Post Code</th>
                                    </tr>
                                    <tr>
                                        <td><%=mailing_address[i].city%></td>
                                        <td><%=mailing_address[i].state%></td>
                                        <td><%=mailing_address[i].road%></td>
                                        <td><%=mailing_address[i].postcode%></td>
                                    </tr>
                                </table>
                            </td>

                        </tr>
                        <%}%>

                        </tbody>
                        <tfoot>
                        <tr>
                            <th style="text-align: center">Mailing Address</th>
                        </tr>
                        </tfoot>
                    </table>

                    <table class="table table-striped table-bordered">
                        <thead>
                        <th style="text-align: center">Permanent Address</th>
                        </thead>
                        <tbody>
                        <% for(var j=0;j<permanentAddress.length;j++){%>
                        <tr>
                            <td>
                                <table class="table table-bordered">
                                    <tr>
                                        <th>District</th>
                                        <th>Upazila</th>
                                        <th>Post Office</th>
                                        <th>Village</th>
                                        <th>Road</th>
                                        <th>Block</th>
                                        <th>House</th>
                                        <th>Post Code</th>
                                    </tr>
                                    <tr>
                                        <td><%=permanentAddress[j].district%></td>
                                        <td><%=permanentAddress[j].upazila%></td>
                                        <td><%=permanentAddress[j].post_office%></td>
                                        <td><%=permanentAddress[j].village%></td>
                                        <td><%=permanentAddress[j].road%></td>
                                        <td><%=permanentAddress[j].block%></td>
                                        <td><%=permanentAddress[j].house%></td>
                                        <td><%=permanentAddress[j].post_code%></td>
                                    </tr>
                                </table>
                            </td>
                        </tr>
                        <%}%>
                        </tbody>
                        <tfoot>
                        <th style="text-align: center">Permanent Address</th>
                        </tfoot>
                    </table>
                    <table class="table table-striped table-bordered">
                        <thead>
                        <th style="text-align: center">Company Information</th>
                        </thead>
                        <tbody>
                            <% for(var j=0;j<company_info.length;j++){%>
                            <tr>
                                <td>
                                    <table class="table table-bordered">
                                        <tr>
                                            <th>Name</th>
                                            <th>Cell</th>
                                            <th>Telephone</th>
                                            <th>Email</th>
                                            <th>Website</th>
                                            <th>Logo</th>
                                        </tr>
                                        <tr>
                                            <td><%=company_info[j].companyname%></td>
                                            <td><%=company_info[j].cellphone%></td>
                                            <td><%=company_info[j].telephone%></td>
                                            <td><%=company_info[j].email%></td>
                                            <td>
                                                <a href="<%=company_info[j].website%>"><%=company_info[j].website%></a>
                                            </td>
                                            <td>
                                                <img src="/uploads/companies/<%=company_info[j].logo%>" alt="" style="width: 100px;height: 100px">
                                            </td>
                                        </tr>
                                    </table>
                                </td>
                            </tr>
                            <%}%>
                        </tbody>
                        <tfoot>
                        <th style="text-align: center">Company information</th>
                        </tfoot>
                    </table>
                </div>
                <div class="col-sm-2">
                </div>
            </div>
        </section>
        <!-- /wrapper -->
    </section>
    <!-- /MAIN CONTENT -->
    <!--main content end-->
    <!--footer start-->
    <footer class="site-footer">
        <div class="text-center">
            <div class="credits">
                Copyright &copy;<script>document.write(new Date().getFullYear());</script> All rights reserved | by <a href="http://softcareit.com/" target="_blank">SoftcareIT</a>
            </div>
            <a href="" class="go-top">
                <i class="fa fa-angle-up"></i>
            </a>
        </div>
    </footer>
    <!--footer end-->
</section>
<!-- js placed at the end of the document so the pages load faster -->
<script src="/admin/lib/jquery/jquery.min.js"></script>
<script src="/admin/lib/bootstrap/js/bootstrap.min.js"></script>
<script class="include" type="text/javascript" src="/admin/lib/jquery.dcjqaccordion.2.7.js"></script>
<script src="/admin/lib/jquery.scrollTo.min.js"></script>
<script src="/admin/lib/jquery.nicescroll.js" type="text/javascript"></script>
<!--common script for all pages-->
<script src="/admin/lib/common-scripts.js"></script>
<script src="https://cdn.datatables.net/1.10.20/js/jquery.dataTables.min.js"></script>
<script src="https://cdn.datatables.net/1.10.20/js/dataTables.bootstrap4.min.js"></script>
<!--script for this page-->

</body>

</html>
