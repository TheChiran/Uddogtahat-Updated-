<!DOCTYPE html>
<html lang="en">

<head>
    <% if(title.length> 0){%>
    <title><%=title[0].title%></title>
    <%} else {%>
        <title></title>
    <%}%>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="description" content="Zeta Template Project">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" type="text/css" href="/styles/bootstrap4/bootstrap.min.css">
    <link href="plugins/fontawesome-free-5.0.1/css/fontawesome-all.css" rel="stylesheet" type="text/css">
    <link rel="stylesheet" type="text/css" href="/plugins/OwlCarousel2-2.2.1/owl.carousel.css">
    <link rel="stylesheet" type="text/css" href="/plugins/OwlCarousel2-2.2.1/owl.theme.default.css">
    <link rel="stylesheet" type="text/css" href="/plugins/OwlCarousel2-2.2.1/animate.css">
    <link rel="stylesheet" type="text/css" href="/styles/main_styles.css">
    <link rel="stylesheet" type="text/css" href="/styles/responsive.css">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
    <link href="https://fonts.googleapis.com/css?family=Droid+Sans:400,700" rel="stylesheet">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/baguettebox.js/1.8.1/baguetteBox.min.css">
    <link rel="stylesheet" href="/styles/thumbnail-gallery.css">
    <script>
        window.onload = function () {
            var next = '';
            var page = parseInt(pageNum());
            next += '<a class="page-link" onclick="return pageNum()"  href="/committee-members/page/' + (page + 1);
            next += '" >Next</a>';
            document.getElementById('pageNext').innerHTML = next;
            var previous = '';
            previous += '<a class="page-link" onclick="return pageNum()"  href="/committee-members/page/' + (page - 1);
            previous += '" >Previous</a>';
            document.getElementById('pagePrevious').innerHTML = previous;
        };
        function pageNum() {
            var pageNum = $('#getPageNum').val();
            return pageNum;
        }
    </script>
    <style>
        .dropbtn {
            font-size: 12px;
            border: none;
            background: none;
            text-transform: uppercase;
            color: #0b1033;
            letter-spacing: 3.5px;
            padding-left: 14px;
            padding-right: 10px;
            font-weight: 600;
        }
        .dropbtn:hover{
            cursor: pointer;
            color: #fff;
        }
        .dropdown {
            display: inline-block;
            margin-top: 45px !important;
        }
        .dropdown-content {
            display: none;
            position: absolute;
            background-color: #28cc28;
            min-width: 160px;
            z-index: 1;
        }

        .dropdown-content a {
            color: #fff;
            padding: 12px 16px;
            text-decoration: none;
            display: block;
        }
        .dropdown-content:hover{
            cursor: pointer;
            color: #fff;
        }
        .dropdown-content a:hover {background-color: #ddd;
            color: #fff;
        }

        .dropdown:hover .dropdown-content {display: block;}

        .dropdown:hover .dropbtn {
            background-color: #28cc28;
            color: #fff;
        }

    </style>
</head>

<body>

<div class="super_container">

    <!-- Header -->

    <header class="header d-flex flex-row justify-content-end align-items-center">

        <!-- Logo -->
        <div class="logo_container mr-auto">
            <div class="logo">
                <% if(logo.length > 0){%>
                    <% for(var i=0;i<logo.length;i++){%>
                    <a href="#"><span><img src="/uploads/logo/<%=logo[i].image%>" alt="" style="height: 80px"></span><span></span></a>
                    <%}%>
                <%} else{%>
                    <a href="#"><span><img src="/uploads/logo/logo.jpg" alt="" style="height: 80px"></span><span></span></a>
                <%}%>
            </div>
        </div>

        <!-- Main Navigation -->

        <% include ../partials/navbar %>
    </header>

    <!-- Menu -->

    <!-- Menu -->

    <div class="fs_menu_overlay"></div>
    <div class="fs_menu_container">
        <div class="fs_menu_shapes"><img src="images/menu_shapes.png" alt=""></div>
        <% include ../partials/hamberger %>

        <div class="fs_social_container d-flex flex-row justify-content-end align-items-center">
            <ul class="fs_social">
                <% if(socialAccounts.length > 0){%>
                    <% for(var i=0;i<socialAccounts.length;i++){%>
                    <li>
                        <a href="<%=socialAccounts[i].link%>">
                            <img src="/uploads/socialAccounts/<%=socialAccounts[i].icon%>" alt="" style="height: 25px;width: 25px">
                        </a>
                    </li>
                    <%}%>
                <%} else{%>
                    <li>
                        <a href="">
                            <img src="/uploads/socialAccounts/" alt="" style="height: 25px;width: 25px">
                        </a>
                    </li>
                <%}%>
            </ul>
        </div>
    </div>

    <!-- Hero Slider -->


    <!--Image Gallery-->
    <div class="container gallery-container">

        <div class="tz-gallery">
            <h1 style="text-transform: uppercase">Committee Members</h1>
            <div class="row mt-5">
                <% if(committees.length > 0){%>

                <input type="text" id="getPageNum" value="<%=page%>" hidden>
                <% for(var i=0;i<committees.length;i++){%>
                <div class="col-sm-12 col-md-4">
                    <div class="thumbnail">
                        <a class="lightbox" href="">
                            <img src="/uploads/committee/<%=committees[i].image%>" alt="Person" style="width: 100%;height: 250px">
                        </a>
                        <div class="caption">
                            <div class="lightbox">
                                <table>
                                    <tr>
                                        <td><i class="fas fa-user"></i></td>
                                        <td><%=committees[i].name%></td>
                                    </tr>
                                </table>
                            </div>
                            <div class="lightbox">
                                <table>
                                    <tr>
                                        <td><i class="fas fa-phone"></i></td>
                                        <td> <%=committees[i].phone%></td>
                                    </tr>
                                </table>
                            </div>
                            <div class="lightbox">
                                <table>
                                    <tr>
                                        <td><i class="fas fa-envelope"></i></td>
                                        <td> <%=committees[i].email%></td>
                                    </tr>
                                </table>
                            </div>
                            <div class="lightbox">
                                <table>
                                    <tr>
                                        <td><i class="fas fa-address-card"></i></td>
                                        <td> <%=committees[i].designation%></td>
                                    </tr>
                                </table>
                            </div>
                            <div class="lightbox">
                                <table>
                                    <tr>
                                        <td><i class="fas fa-briefcase"></i></td>
                                        <td> <%=committees[i].companyname%></td>
                                    </tr>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
                <%}%>
                <%} else {%>
                <div class="col-sm-12 col-md-4">
                    <div class="thumbnail">
                        <a class="lightbox" href="">
                            <img src="/uploads/committee/" alt="Person" style="width: 100%;height: 250px">
                        </a>
                        <div class="caption">
                            <div class="lightbox">
                                <table>
                                    <tr>
                                        <td><i class="fas fa-user"></i></td>
                                        <td></td>
                                    </tr>
                                </table>
                            </div>
                            <div class="lightbox">
                                <table>
                                    <tr>
                                        <td><i class="fas fa-phone"></i></td>
                                        <td></td>
                                    </tr>
                                </table>
                            </div>
                            <div class="lightbox">
                                <table>
                                    <tr>
                                        <td><i class="fas fa-envelope"></i></td>
                                        <td></td>
                                    </tr>
                                </table>
                            </div>
                            <div class="lightbox">
                                <table>
                                    <tr>
                                        <td><i class="fas fa-address-card"></i></td>
                                        <td></td>
                                    </tr>
                                </table>
                            </div>
                            <div class="lightbox">
                                <table>
                                    <tr>
                                        <td><i class="fas fa-briefcase"></i></td>
                                        <td></td>
                                    </tr>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
                <%}%>
            </div>

        </div>

    </div>
    <!--gallery Pagination-->
    <div class="container">
        <div class="row">
            <div class="col-md-4"></div>
            <input type="text" value="" id="getPageNum" hidden>
            <div class="col-md-4 col-sm-4">
                <nav aria-label="Page navigation example">
                    <ul class="pagination">
                        <li class="page-item" id="pagePrevious"></li>
                        <% for(var i=0;i<pages;i++){%>
                            <% if (page== i+1) { %>
                            <li class="page-item active"><a style="color: #fff !important;" class="page-link" href="/committee-members/page/<%=(i+1)%>"><%=(i+1)%></a></li>
                            <%} else { %>
                            <li class="page-item"><a class="page-link" href="/committee-members/page/<%=(i+1)%>"><%=(i+1)%></a></li>
                            <% } %>
                        <%}%>
                        <li class="page-item" id="pageNext"></li>
                    </ul>
                </nav>
            </div>
            <div class="col-md-4"></div>
        </div>
    </div>






    <!-- Footer -->

    <footer class="footer">
        <div class="container">
            <div class="row footer_content d-flex flex-sm-row flex-column align-items-center">
                <div class="col-sm-6 cr text-sm-left text-center">
                    <p>
                        <!-- Link back to Colorlib can't be removed. Template is licensed under CC BY 3.0. -->
                        Copyright &copy;<script>
                            document.write(new Date().getFullYear());
                        </script> All rights reserved | by <a href="https://softcareit.com/" target="_blank">SoftCareIt</a>
                        <!-- Link back to Colorlib can't be removed. Template is licensed under CC BY 3.0. -->
                    </p>
                </div>
                <div class="col-sm-6 text-sm-right text-center">
                    <div class="footer_social_container">
                        <ul class="fs_social">
                            <% if(socialAccounts.length > 0){%>
                                <% for(var i=0;i<socialAccounts.length;i++){%>
                                <li>
                                    <a href="<%=socialAccounts[i].link%>">
                                        <img src="/uploads/socialAccounts/<%=socialAccounts[i].icon%>" alt="" style="height: 25px;width: 25px">
                                    </a>
                                </li>
                                <%}%>
                            <%} else{%>
                                <li>
                                    <a href="">
                                        <img src="/uploads/socialAccounts/" alt="" style="height: 25px;width: 25px">
                                    </a>
                                </li>
                            <%}%>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
    </footer>

</div>
<script>
    function myFunction() {
        var x = document.getElementById("myTopnav");
        if (x.className === "topnav") {
            x.className += " responsive";
        } else {
            x.className = "topnav";
        }
    }
</script>
<script src="/js/jquery-3.2.1.min.js"></script>
<script src="/styles/bootstrap4/popper.js"></script>
<script src="/styles/bootstrap4/bootstrap.min.js"></script>
<script src="/plugins/greensock/TweenMax.min.js"></script>
<script src="/plugins/greensock/TimelineMax.min.js"></script>
<script src="/plugins/scrollmagic/ScrollMagic.min.js"></script>
<script src="/plugins/greensock/animation.gsap.min.js"></script>
<script src="/plugins/greensock/ScrollToPlugin.min.js"></script>
<script src="/plugins/progressbar/progressbar.min.js"></script>
<script src="/plugins/OwlCarousel2-2.2.1/owl.carousel.js"></script>
<script src="/plugins/easing/easing.js"></script>
<script src="/js/custom.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/baguettebox.js/1.8.1/baguetteBox.min.js"></script>
<script>
    baguetteBox.run('.tz-gallery');
</script>
</body>

</html>
